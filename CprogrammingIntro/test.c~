#include <stdio.h>
int main()
{
  int acheck, sum = 0;
  while 
                    (scanf("%d%*c",&acheck)!=EOF)
                        {
                        if (acheck>5) {
                            printf("Tat may %d thanh cong !\n",acheck);
                            }
                        else printf("Khong hop le.May %d dang tat.\n",acheck);
                    }
  printf("Sum is %d\n", sum);
  return 0;
}
