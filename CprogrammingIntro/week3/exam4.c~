#include<stdio.h>
int main(){
  int *ptr;
  int x=12345,y;
  ptr=&x;
  printf("The value of ptr is %p\n",ptr);
  printf("The address of x is %p\n\n",&x);
  printf("Total charac on this line: %n",&y);
  printf("%d\n\n",y);
  y=printf("This line has 28 charac\n");
  printf("%d charac were printed\n\n");
  printf("print a %% in a format control string");
  return 0;

}
