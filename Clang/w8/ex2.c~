#include<stdio.h>
#include<math.h>
int main(){
  int n,i,ch=1;
  scanf("%d",&n);
  n=n+1;
  while (ch==1){
    int dem=0;
    for (i=2;i<=(int)sqrt(n)&&dem==0;i++){
        if (n % i ==0)
      dem=dem+1;
    
  }
    if (dem==0)
      {
	printf("SNT %d\n",n);
	ch=0;
      }
    n=n+1;
  }
  
  /// goi ham...
  return 0;

}  
